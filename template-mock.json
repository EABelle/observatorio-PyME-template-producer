        {
          "type":"SELECT",
          "title":"Actividad principal",
          "options":[
            "Comercialización al exterior",
            "Fábrica",
            "Construcción"
          ],
          "description":"(*) Si la empresa factura por horas o distancia recorrida",
          "mandatory":true
        },
        {
          "type":"CHOICE",
          "title":"Realizó inversiones durante 2019?",
          "options":[
            "Sí",
            "No"
          ],
          "mandatory":true
        },
        {
          "type":"FILE",
          "title":"Archivo 1",
          "mandatory":true
        },
        {
          "type":"TEXT",
          "title":"¿En qué invirtió?",
          "multiline":true
        },
        {
          "type":"GROUPED",
          "title":"¿En qué invirtió?",
          "questions":[
            {
              "type":"TEXT",
              "title":"Reinversión de utilidades",
              "adornment":"%"
            },
            {
              "type":"TEXT",
              "title":"Aportes",
              "adornment":"%"
            },
            {
              "type":"TEXT",
              "title":"Programas públicos",
              "adornment":"%"
            },
            {
              "type":"TEXT",
              "title":"Otros",
              "adornment":"%"
            },
            {
              "type":"TEXT",
              "title":"Especificar \"Otros\""
            }
          ]
        },
        {
          "type":"NUMBER",
          "title":"¿En qué año invirtió?",
          "description":"Min: 1980 / Max: 2020",
          "restrictions":{
            "min":1980,
            "max":2020
          }
        }
      ]
    }
  ]
